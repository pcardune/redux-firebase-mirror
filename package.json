{
  "name": "redux-firebase-mirror",
  "version": "0.0.1",
  "description": "A library to help you easily mirror firebase data inside a redux store.",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:pcardune/redux-firebase-mirror.git",
    "type": "git"
  },
  "author": "Paul Carduner <paul@carduner.net>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "babel -d dist src/",
    "docs": "documentation build -f html -o docs src/**/*.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupJasmineEnv.js",
    "coverageDirectory": "./reports/coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "documentation": "^4.0.0-beta15",
    "eslint": "^3.11.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-react": "^6.7.1",
    "firebase": "^3.6.2",
    "flow-bin": "^0.36.0",
    "flow-typed": "^2.0.0",
    "jasmine-reporters": "^2.2.0",
    "jest": "^17.0.3",
    "react": "^15.4.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "redux-immutable": "^3.0.8",
    "reselect": "^2.5.4"
  },
  "peerDependencies": {
    "firebase": "^3.6.2",
    "react": "^15.4.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
