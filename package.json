{
  "name": "redux-firebase-mirror",
  "version": "0.0.1",
  "description": "A library to help you easily mirror firebase data inside a redux store.",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:pcardune/redux-firebase-mirror.git",
    "type": "git"
  },
  "author": "Paul Carduner <paul@carduner.net>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "prepublish": "npm run build",
    "lint": "eslint src/ examples/",
    "build": "babel --ignore *.test.js -d dist src/",
    "docs": "documentation build -f html -o docs src/**/*.js"
  },
  "files": [
    "README.md",
    "LICENSE",
    "index.js",
    "dist/*"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./setupJasmineEnv.js",
    "coverageDirectory": "./reports/coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "documentation": "^4.0.0-beta15",
    "enzyme": "^2.8.0",
    "eslint": "^3.11.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-react": "^6.7.1",
    "firebase": "^3.6.2",
    "flow-bin": "^0.43.1",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-reporters": "^2.2.0",
    "jest": "^19.0.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.1.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "firebase": "^3.6.2",
    "immutable": "^3.8.1",
    "redux-immutable": "^4.0.0",
    "reselect": "^3.0.0"
  },
  "optionalDependencies": {
    "react": "^15.4.1",
    "react-redux": "^5.0.3"
  },
  "peerDependencies": {
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
