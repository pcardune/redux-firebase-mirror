{
  "name": "redux-firebase-mirror",
  "version": "0.0.12",
  "description": "A library to help you easily mirror firebase data inside a redux store.",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:pcardune/redux-firebase-mirror.git",
    "type": "git"
  },
  "author": "Paul Carduner <paul@carduner.net>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "prepare": "npm run build",
    "lint": "eslint src/ examples/",
    "build": "rm -rf dist && babel --ignore '*.test.js' -d dist src/",
    "build:watch": "nodemon --watch src --exec npm run build",
    "docs": "documentation build -f html -o docs src/**/*.js",
    "prettier": "prettier --single-quote --trailing-comma es5 --no-bracket-spacing --write src/**/*.js src/*.js examples/*.js examples/*.jsx"
  },
  "files": [
    "README.md",
    "LICENSE",
    "index.js",
    "dist/*"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./setupJasmineEnv.js",
    "coverageDirectory": "./reports/coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "documentation": "^5.3.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-react": "^7.4.0",
    "firebase": "^4.5.0",
    "flow-bin": "^0.56.0",
    "flow-typed": "^2.2.0",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-reporters": "^2.2.1",
    "jest": "^23.4.2",
    "nodemon": "^1.12.1",
    "prop-types": "^15.6.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.3",
    "react-test-renderer": "^16.0.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "lodash.memoize": "^4.1.2",
    "redux-immutable": "^4.0.0",
    "reselect": "^3.0.1"
  },
  "optionalDependencies": {
    "react": "^16.0.0",
    "react-redux": "^5.0.6"
  },
  "peerDependencies": {
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
